    "@feathersjs/socketio": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "faker": "^5.1.0",
    "feathers-nedb": "^5.1.0",
    "feathers-objection": "^7.1.0",
